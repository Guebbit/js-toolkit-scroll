{
  "name": "@guebbit/js-toolkit",
  "version": "1.0.1",
  "license": "AGPLv3.0",
  "sideEffects": false,
  "scripts": {
    "doc": "",
    "build": "tsc",
    "test": "jest",
    "test:target": "jest /tests/setLazyload.test.ts",
    "lint": "eslint \"*/**/*.{ts,js,json}\"",
    "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "docs:dev": "vitepress dev --port 8080",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "prepublish": "npm run build && npm run test && npm run lint && npm run docs:build",
    "publish:public": "npm run prepublish && npm publish --access public"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitepress": "^1.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepublish"
    }
  },
  "dependencies": {
    "@guebbit/js-toolkit": "^1.0.1",
    "lodash": "^4.17.21"
  }
}
